    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo.owl#Time -->

    <owl:Class rdf:about="&gfo;Time">
        <rdfs:label xml:lang="en">Time entity</rdfs:label>
        <rdfs:subClassOf rdf:resource="&gfo;Space_time"/>
