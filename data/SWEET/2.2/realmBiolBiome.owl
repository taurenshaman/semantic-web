        <owl:hasValue rdf:resource="&zone;#Dfc" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&zone;#hasClimateZone" />
        <owl:hasValue rdf:resource="&zone;#Dfb" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">biome characterized by coniferous forests</rdfs:comment>
  </owl:Class>

