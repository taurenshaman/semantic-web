    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Biota">
    <rdfs:subClassOf rdf:resource="#TerrestrialBiome"/>
  </owl:Class>

  <owl:Class rdf:about="#Croplands">
    <rdfs:subClassOf rdf:resource="#TerrestrialBiome"/>
  </owl:Class>

  <owl:Class rdf:about="#DenseSettlements">
    <rdfs:subClassOf rdf:resource="#TerrestrialBiome"/>
  </owl:Class>

  <owl:Class rdf:about="#Forested">
    <rdfs:subClassOf rdf:resource="#TerrestrialBiome"/>
  </owl:Class>

  <owl:Class rdf:about="#Rangelands">
    <rdfs:subClassOf rdf:resource="#TerrestrialBiome"/>
  </owl:Class>

  <owl:Class rdf:about="#Terrain">
    <rdfs:subClassOf rdf:resource="#TerrestrialBiome"/>
  </owl:Class>

  <owl:Class rdf:about="#Villages">
    <rdfs:subClassOf rdf:resource="#TerrestrialBiome"/>
  </owl:Class>

  <owl:Class rdf:about="#Wildlands">
