  </owl:Class>

  <owl:Class rdf:about="#Radioelement">
    <rdfs:subClassOf rdf:resource="&elem;Element"/>
    <rdfs:subClassOf rdf:resource="#RadioactiveSubstance"/>
  </owl:Class>

  <owl:ObjectProperty rdf:about="#halfLife">
