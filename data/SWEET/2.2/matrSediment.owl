  <!ENTITY sed "http://sweet.jpl.nasa.gov/2.2/matrSediment.owl">
  <!ENTITY his "http://sweet.jpl.nasa.gov/2.2/reprSciProvenance.owl">
  <!ENTITY solid "http://sweet.jpl.nasa.gov/2.2/phenSolid.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&sed;"
         xmlns:owl="&owl;"
         xmlns:chem="&chem;"
         xmlns:solid="&solid;"
         xmlns:sed="&sed;"
         xmlns:his="&his;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="1.1">
     <owl:imports rdf:resource="&chem;"/>
     <owl:imports rdf:resource="&his;"/>
     <owl:imports rdf:resource="&solid;"/>
  </owl:Ontology>

<!-- Substances -->

  <owl:Class rdf:about="#Alluvium">
    <rdfs:subClassOf rdf:resource="#Sediment"/>
  </owl:Class>

  <owl:Class rdf:about="#Bentonite">
    <rdfs:subClassOf rdf:resource="#Clay"/>
  </owl:Class>

  <owl:Class rdf:about="#Boulder">
